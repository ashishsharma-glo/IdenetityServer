@model WebApp.Models.VerifyOneTouchViewModel
<h2>Please Authenticate</h2>

<i class="fa fa-spinner fa-pulse"></i> Waiting for <i>OneTouch</i> approval

@using (Html.BeginForm("VerifyOneTouch", "Account"))
{
    @Html.AntiForgeryToken()
    @Html.Hidden("provider", @Model.Provider)
    @Html.Hidden("rememberMe", @Model.RememberMe)
}


<script src="~/Scripts/jquery-1.10.2.min.js"></script>

<script src="~/Scripts/onetouch.js"></script>



